ANTHROPIC_API_KEY=your-z-ai-api-key
ANTHROPIC_AUTH_TOKEN=your-z-ai-api-key
ANTHROPIC_BASE_URL=https://api.z.ai/api/anthropic

# Database
DATABASE_URL=postgresql://localhost:5432/computetower

